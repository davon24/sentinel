#file:/etc/systemd/system/sentinel.service
[Unit]
Description=Sentinel Sentry Service
Documentation=

[Service]
Restart=always
User=
Environment="LD_LIBRARY_PATH=/opt/sentinel/Python-3.8.6/lib"
Environment="PATH=/opt/sentinel/Python-3.8.6/bin:/usr/bin:/usr/sbin:/bin:/sbin"
WorkingDirectory=/opt/sentinel/python
ExecStart=/opt/sentinel/python/sentinel.py sentry

[Install]
WantedBy=multi-user.target

