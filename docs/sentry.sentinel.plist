<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd";>
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>sentry.sentinel</string>
    <key>Program</key>
    <string>/opt/sentinel/python/sentinel.py</string>
    <key>WorkingDirectory</key>
    <string>/opt/sentinel/python</string>
    <key>ProgramArguments</key>
    <array>
      <string>/opt/sentinel/python/sentinel.py</string>
      <string>sentry</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <false/>
    </dict> 
<key>EnvironmentVariables</key>
<dict>
  <key>PATH</key>
  <string>/usr/local/bin:/sbin:/usr/sbin:/bin:/usr/bin</string>
  <key>PYTHONPATH</key>
  <string>/opt/sentinel/python/site-packages</string>
</dict>
<key>StandardErrorPath</key>
<string>/tmp/sentinel-error.log</string>
<key>StandardOutPath</key>
<string>/tmp/sentinel-info.log</string>
  </dict>
</plist>
